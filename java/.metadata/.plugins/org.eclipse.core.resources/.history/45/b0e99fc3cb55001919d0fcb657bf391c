
public class Tower {
	ArrayList<Disc> disc = new ArrayList<String>();

	public void draw() {
		for (int i = 6; i > 0; i--) {
			System.out.println("|");
		}

		if (disc != null) {
			disc.draw();
		}
	}

	public void addDisc(Disc disc) {
		this.disc = disc;
	}

	public Disc removeDisc() {
		return null;

	}
}
