import java.util.ArrayList;

public class Tower {
	ArrayList<Disc> disc = new ArrayList<Disc>();

	public void draw() {
		for (int i = 6; i > 0; i--) {
			System.out.println("|");
		}

		for (Disc i : disc) {
			System.out.println(i);

		}
	}

	public void addDisc(ArrayList<Disc> disc) {
		this.disc = disc;
	}

	public Disc removeDisc() {
		return null;

	}
}
